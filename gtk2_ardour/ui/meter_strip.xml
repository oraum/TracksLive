<?xml version="1.0" encoding="UTF-8"?>

<RouteUI selectedfrontopacity="0.8"
		 selectedfrontcolor="#C0C0C0"
	     bgnormal="#424242"
		 bgactive="#424242"
		 namebgactive="#FFFFFF"
		 namefgactive="#000000"
		 namefgnormal="#FFFFFF"
		 gainmeterscript="meter_strip_gain_meter.xml">
  <EventBox id="panners_home"
            ui.orphan="true"/>
	<VBox>
	<HBox>
		<EventBox id="frame"
			bgnormal="#424242"
			bgactive="#FFFFFF">
    <EventBox id="pad"
			  bgnormal="#383838"
			  bgactive="#424242"
		 borderwidth="1">
      <VBox>
        <HBox id="level_meter_home"/>
        <Vbox>
          <HBox id="gain_meter_home"/>
            <VBox>
              <HBox>
                <EventBox id="rec_enable_indicator_on"
                          width="38"
                          height="3"
                          bgnormal="#ED1C24"
                          bgactive="#ED1C24"
                          visible="false"
                          noshowall="true"/>
                <EventBox id="rec_enable_indicator_off"
                          width="38"
                          height="3"
                          bgnormal="#000000"
                          bgactive="#000000"
                          visible="false"
                          noshowall="true"/>
                <Button id="rec_enable_button"
                        width="19"
                        height="13"
                        ui.orphan="true"/>
                <Button id="monitor_input_button"
                        width="19"
                        height="13"
                        ui.orphan="true"/>
              </HBox>
              <HBox>
                <Button id="mute_button"
                            width="19"
                            height="13"
                            ui.orphan="true"/>
                <Button id="solo_button"
                        width="19"
                        height="13"
                        ui.orphan="true"/>
              </HBox>

              <EventBox id="name_home"
				  bgactive="#FFFFFF">
                <HBox>
                  <Label id="name_label"
                          width="38"
                          height="24"
                          fgnormal="#FFFFFF"
                          fgactive="#000000"
                          fghover="#FFFFFF"
                          winfont="Arial 8"
                          macfont="Helvetica 8"/>
                  <VBox id="name_entry_eventbox"
                            width="38"
                            height="24"
                            visible="false">
                    <FocusEntry id="name_entry"
                                fgnormal="#ffffff"
                                winfont ="Arial 8"
                                macfont ="Helvetica 8"
                                textcolornormal="#ffffff"
                                textcoloractive="#ffffff"
                                textcolorselected="#ffffff"
								basecoloractive="#424242"
                                noshowall="true"
                                visible="false"
					  			hasframe="false"
								box.expand="true"/>
                  </VBox>
                </HBox>
              </EventBox>
            </VBox>
        </Vbox>
      </VBox>
    </EventBox>
		</EventBox>
    <EventBox bgnormal="#000000"
              bgactive="#000000"
              width="1"/>
  </HBox>
        <EventBox bgnormal="#000000"
		bgactive="#000000"
		height="2"
		box.pack="end"/>
  </VBox>
  <Button id="show_sends_button" ui.orphan="true"/>
  <Button id="monitor_disk_button" ui.orphan="true"/>
  <Button id="comment_button" ui.orphan="true"/>
  <Button id="midi_input_enable_button" ui.orphan="true"/>
  <Button id="group_button" text="Grp" ui.orphan="true"/>
  <HBox id="invert_button_box" ui.orphan="true"/>
  <button id="master_mute_button" ui.orphan="true"/>
  <VBox id="track_buttons_home" ui.orphan="true"/>
</RouteUI>
