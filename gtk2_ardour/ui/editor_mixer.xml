                                  y="13"
                                  width="80"
                                  height="20"
                                  fgnormal="#ffffff"
                                  winfont ="Arial 10"
                                  macfont ="Helvetica 10"
                                  textcolornormal="#ffffff"
                                  textcoloractive="#ffffff"
                                  textcolorselected="#ffffff"
                                  basecolornormal="#424242"
                                  noshowall="true"
                                  visible="false"
                                  box.expand="true"
                                  hasframe="false"/>
