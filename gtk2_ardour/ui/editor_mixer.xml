                    height="1"/>
          <ScrolledWindow box.expand="true"
                          box.fill="true"
                          bgnormal="#424242">
            <VBox id="info_panel_home"
                  noshowall="true"
                  visible="false">
              <EventBox bgnormal="#4F4F4F">
                <VBox>
                  <HBox height="4"/>
                  <HBox>
